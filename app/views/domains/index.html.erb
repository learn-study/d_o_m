<div class="container">
<h1>Domains#index</h1>
   <table class="table table-striped table-bordered table-responsive">
	<thead>
		<tr>
			<th>Domain</th>
			<th>PR</th>
			<th>BL</th>
			<th>DP</th>
			<th>ABY</th>
			<th>SimilarWeb</th>
			<th>Dmoz</th>
			<th>C</th>
			<th>N</th>
			<th>O</th>
			<th>D</th>
			<th>EndDate</th>		</tr>
	</thead>
   <tbody>
   <% @domains.each do |item| %>
   		<tr>
            <th ><%= item.name %></th>
            <td><%= item.pr %></td>
            <td><%= item.bl %></td>
            <td><%= item.dp %></td>
            <td><%= item.aby %></td>
            <td><%= item.smilerweb %></td>
            <td><%= item.dmoz %></td>
            <td><%= item.c %></td>
            <td><%= item.n %></td>
            <td><%= item.o %></td>
            <td><%= item.d %></td>
            <td><%= item.dropped %></td>
        </tr>
   <% end %>
   </tbody>
   </table>
</div>